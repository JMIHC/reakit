{
  "name": "reakit",
  "version": "0.16.0",
  "description": "Toolkit for building accessible UIs with React",
  "license": "MIT",
  "repository": "https://github.com/reakit/reakit",
  "main": "lib/index.js",
  "module": "es/index.js",
  "unpkg": "dist/reakit.min.js",
  "types": "ts",
  "author": {
    "name": "Diego Haz",
    "email": "hazdiego@gmail.com",
    "url": "https://github.com/diegohaz"
  },
  "scripts": {
    "test": "jest",
    "coverage": "yarn test --coverage",
    "postcoverage": "opn coverage/lcov-report/index.html",
    "type-check": "tsc --noEmit",
    "lint": "eslint . --ext js,ts,tsx",
    "build:clean": "rimraf dist es lib ts && node scripts/cleanProxies",
    "build:ts": "tsc --emitDeclarationOnly",
    "sprebuild": "yarn build:clean && yarn build:ts",
    "build": "../../scripts/build/build.js",
    "dev": "echo 'TODO'",
    "preversion": "yarn lint && yarn test && yarn build",
    "postpublish": "yarn build:clean"
  },
  "keywords": [
    "reakit",
    "a11y",
    "react",
    "ui",
    "toolkit",
    "components"
  ],
  "dependencies": {
    "popper.js": "^1.14.7"
  },
  "peerDependencies": {
    "react": "^16.8.0",
    "react-dom": "^16.8.0"
  }
}
